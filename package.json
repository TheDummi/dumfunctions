{
    "name": "dumfunctions",
    "version": "2.1.1",
    "description": "A package for JS functions that should be in the main language but aren't.",
    "main": "dist/main.js",
    "type": "module",
    "exports": {
        "import": "./dist/main.js",
        "require": "./dist/main.cjs",
        "types": "./types",
        "default": null,
        "node": null
    },
    "scripts": {
        "start": "npm i && npm audit fix --force && npm fund && tsc -w",
        "build": "tsc && npx tsup src/main.ts && npx prettier --write src"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/TheDummi/DummiFuncs.git"
    },
    "keywords": [
        "JSfunctions"
    ],
    "author": "Dummi",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/TheDummi/DummiFuncs/issues"
    },
    "homepage": "https://github.com/TheDummi/DummiFuncs#readme",
    "devDependencies": {
        "prettier": "^2.8.4",
        "tsup": "^6.6.0",
        "typescript": "^4.9.5"
    }
}
